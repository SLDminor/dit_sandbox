{
  "name": "todolistapp",
  "version": "1.0.0",
  "description": "My ToDoList App",
  "author": "Gromyko Gregory",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build todo-lib --configuration production",
    "version:lib": "cd projects/todo-lib && npm version",
    "publish:lib": "npm run build:lib && cd dist/todo-lib && npm publish --access public"
  },
  "dependencies": {
    "@angular/common": "~20.0.0",
    "@angular/compiler": "~20.0.0",
    "@angular/core": "~20.0.0",
    "@angular/forms": "~20.0.0",
    "@angular/platform-browser": "~20.0.0",
    "@angular/router": "~20.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.0.0",
    "@angular/build": "^20.0.6",
    "@angular/cli": "~20.0.0",
    "@angular/compiler-cli": "~20.0.0",
    "ng-packagr": "^20.0.0",
    "typescript": "~5.8.0"
  }
}
